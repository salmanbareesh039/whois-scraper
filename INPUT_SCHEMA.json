{
  "title": "Input schema for Whois Lookup Scraper",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "domains": {
      "title": "Domains",
      "type": "array",
      "items": { "type": "string" },
      "description": "List of domains to lookup",
      "editor": "textarea",
      "prefill": [
        "example.com",
        "apify.com",
        "github.com"
      ]
    }
  },
  "required": ["domains"],
  "examples": [
    {
      "domains": [
        "example.com",
        "apify.com",
        "github.com"
      ]
    }
  ]
}
